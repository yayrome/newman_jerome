{
  "presets": [
   [
     "@babel/preset-env",
     {
       "targets": {
         "browsers": [
           "last 2 versions",
           "IE >= 11"
         ]
       },
       "useBuiltIns": true
     }
   ],
   "@babel/preset-react"
 ],
 "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  ]
}
